#alias ssh 'ssh -YC -c blowfish'
alias ssh 'ssh -Y'
alias ls 'ls --color=auto'
alias l 'ls -A'
alias lc 'ls -1A | wc -l'
alias ll 'ls -Al'
alias l1 'ls -A1'
alias u 'cd ..'
alias new 'ls -Alt \!* | head'
alias old 'ls -Alrt \!* | head'
alias dept 'cd /dept/is/prodsoft'
alias sync-dev-from-san 'rsync --stats --progress --verbose --archive --whole-file /san/dept/is/dev/ahuang/infosys/prodsoft/lib/\!:1 .'
alias sync-env-from-san 'rsync --stats --progress --verbose --archive --whole-file /san/home/ahuang/etc .'
alias tree 'tree -aA'
alias from-trunk 'set _PWD=`pwd` && set _BASE=`basename ${_PWD}` && cd ../../.. && integrate-branch $_BASE && cd -'
alias to-trunk 'set _PWD=`pwd` && set _BASE=`basename ${_PWD}` && cd ../../.. && integrate-branch -r $_BASE && cd -'
alias grep 'grep -IE'
alias v 'vim'
alias x 'exit'
alias pgrep 'ps aux | /bin/grep'
alias mkdirs 'mkdir -p'
alias cpr 'cp -rp'
alias groupusers 'getent group'
alias ss 'source ${HOME}/.cshrc.user'
alias diff "diff --exclude '*.pyc' --exclude '*.pyo' -I ' Revision-Info: ' -I 'ilmversion  = ' -I ' * Revision:' -I ' * CVS:' -I ' * Modified:'"
alias ps 'ps -AF f'
alias c 'cat'
alias raydiff 'diff -rb ${PS}/lib/raygun /opt/infosys/raygun/local/raygun'
alias pewpewlog 'tmux attach-session -t pewpew'
alias rsink 'sudo rsync -auvPWh --stats'
